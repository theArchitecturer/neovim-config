snippet if "if condition"
        if ${1:/* condition */} {
                ${2:${VISUAL}}
                    
        }


snippet ife "if else condition"
        if ${1:/* condition */} {
                ${2:${VISUAL}}
                    
        } else {
                ${0}
                    
        }

snippet iel "if error not nil, log error"
        if err != nil {
            log.Println(err)
        }
